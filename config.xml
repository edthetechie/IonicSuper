<?xml version='1.0' encoding='utf-8'?>
<widget id="com.shapingtomorrow.athenaionic" version="1.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ST Dev (Ionic)</name>
    <description>
        Find the future fast by automatically scanning sources
    </description>
    <author email="tech@cc.uk.com" href="http://shapingtomorrow.com">
        Shaping Tomorrow
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="10" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreen" value="splash" />
    <preference name="android-permission" value="BILLING" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <gap:plugin name="org.apache.cordova.battery-status" source="npm" />
    <gap:plugin name="org.apache.cordova.camera" source="npm" />
    <gap:plugin name="org.apache.cordova.console" source="npm" />
    <gap:plugin name="org.apache.cordova.contacts" source="npm" />
    <gap:plugin name="org.apache.cordova.device" source="npm" />
    <gap:plugin name="org.apache.cordova.device-motion" source="npm" />
    <gap:plugin name="org.apache.cordova.device-orientation" source="npm" />
    <gap:plugin name="org.apache.cordova.dialogs" source="npm" />
    <gap:plugin name="org.apache.cordova.file" source="npm" />
    <gap:plugin name="org.apache.cordova.file-transfer" source="npm" />
    <gap:plugin name="org.apache.cordova.geolocation" source="npm" />
    <gap:plugin name="org.apache.cordova.globalization" source="npm" />
    <gap:plugin name="org.apache.cordova.inappbrowser" source="npm" />
    <gap:plugin name="org.apache.cordova.media" source="npm" />
    <gap:plugin name="org.apache.cordova.media-capture" source="npm" />
    <gap:plugin name="org.apache.cordova.network-information" source="npm" />
    <gap:plugin name="org.apache.cordova.splashscreen" source="npm" />
    <gap:plugin name="org.apache.cordova.vibration" source="npm" />
    <gap:plugin name="org.apache.cordova.statusbar" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <icon src="src/assets/imgs/icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="src/assets/imgs/res/icon/android/icon-36-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="src/assets/imgs/res/icon/android/icon-48-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="src/assets/imgs/res/icon/android/icon-72-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="src/assets/imgs/res/icon/android/icon-96-xhdpi.png" />
    <icon gap:platform="blackberry" src="src/assets/imgs/res/icon/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="src/assets/imgs/res/icon/blackberry/icon-80.png" />
    <icon gap:platform="ios" height="57" src="src/assets/imgs/res/icon/ios/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="src/assets/imgs/res/icon/ios/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="src/assets/imgs/res/icon/ios/icon-57-2x.png" width="114" />
    <icon gap:platform="ios" height="144" src="src/assets/imgs/res/icon/ios/icon-72-2x.png" width="144" />
    <icon gap:platform="ios" height="76" src="src/assets/imgs/res/icon/ios/icon-76.png" width="76" />
    <icon gap:platform="ios" height="120" src="src/assets/imgs/res/icon/ios/icon-120.png" width="120" />
    <icon gap:platform="ios" height="152" src="src/assets/imgs/res/icon/ios/icon-152.png" width="152" />
    <icon gap:platform="webos" src="src/assets/imgs/res/icon/webos/icon-64.png" />
    <icon gap:platform="winphone" src="src/assets/imgs/res/icon/windows-phone/icon-48.png" />
    <icon gap:platform="winphone" gap:role="background" src="src/assets/imgs/res/icon/windows-phone/icon-173-tile.png" />
    <gap:splash src="src/assets/imgs/splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="src/assets/imgs/res/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="src/assets/imgs/res/screen/android/screen-mdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="src/assets/imgs/res/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="src/assets/imgs/res/screen/android/screen-xhdpi-portrait.png" />
    <gap:splash gap:platform="blackberry" src="src/assets/imgs/res/screen/blackberry/screen-225.png" />
    <gap:splash gap:platform="ios" height="480" src="src/assets/imgs/res/screen/ios/screen-iphone-portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="src/assets/imgs/res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="src/assets/imgs/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="src/assets/imgs/res/screen/ios/screen-ipad-portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="src/assets/imgs/res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <gap:splash gap:platform="winphone" src="src/assets/imgs/res/screen/windows-phone/screen-portrait.jpg" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
    <engine name="ios" spec="~4.0.1" />
</widget>
